<html>
    <head>
        <script type="text/javascript" src="//ajax.googleapis.com/ajax/libs/jquery/1.8.2/jquery.min.js"></script>
        <script type="text/javascript">
        $(document).ready(function(){
        });
        </script>

    </head>
    <body>
        <table border=1>
        <tr>
            <th>Time</th>
            <th>Ambient</th>
            <th>Surface</th>
            <th>Snow Height</th>
        </tr>
        {% for r in readings %}
        <tr>
            <td>{{ r.timestamp|time }}</td>
            <td>{{ r.ambient_temp|default_if_none:"?" }}</td>
            <td>{{ r.surface_temp|default_if_none:"?" }}</td>
            <td>{{ r.snow_height|default_if_none:"?" }}</td>
        </tr>
        {% endfor %}
        </table>
    </body>
</html>
